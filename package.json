{
  "name": "pets",
  "version": "1.0.0",
  "description": "pets",
  "scripts": {
    "listphotos": "ls ./src/photos > ./src/photos.txt",
    "dev": "concurrently \"serve -d src\" \"yarn dev:build\"",
    "dev:build": "yarn listphotos && esbuild ./src/index.js --bundle --minify --watch --outfile=./src/bundle.js",
    "build": "yarn listphotos && esbuild ./src/index.js --bundle --minify --outfile=./src/bundle.js",
    "deploy": "gh-pages deploy -d src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadhavMakkena/pets.git"
  },
  "author": "madhav.makkena@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^6.0.2",
    "esbuild": "^0.11.14",
    "eslint": "^7.25.0",
    "gh-pages": "^3.1.0",
    "serve": "^11.3.2"
  },
  "dependencies": {
    "glob": "^7.1.6"
  }
}
